{
  "expo": {
    "name": "YTSummarizer",
    "slug": "YTSummarizer2-react-native-expo-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "scheme": "ytsummarizer",
    "ios": {
      "supportsTablet": true,
      "associatedDomains": [
        "applinks:youtube.com",
        "applinks:m.youtube.com",
        "applinks:youtu.be"
      ],
      "infoPlist": {
        "CFBundleURLTypes": [
          {
            "CFBundleURLSchemes": [
              "ytsummarizer"
            ]
          }
        ],
        "LSApplicationQueriesSchemes": [
          "youtube",
          "http",
          "https"
        ],
        "NSUserActivityTypes": [
          "INSendMessageIntent",
          "INSearchForMessagesIntent"
        ],
        "NSPhotoLibraryAddUsageDescription": "This app needs access to save images to your photo library",
        "NSPhotoLibraryUsageDescription": "This app needs access to your photo library",
        "UIBackgroundModes": [
          "fetch",
          "remote-notification"
        ],
        "UISupportsDocumentBrowser": true,
        "UIFileSharingEnabled": true
      },
      "usesIcloudStorage": false,
      "bundleIdentifier": "com.chirag127.YTSummarizer2reactnativeexpoapp"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.chirag127.YTSummarizer2reactnativeexpoapp",
      "intentFilters": [
        {
          "action": "VIEW",
          "autoVerify": true,
          "data": [
            {
              "scheme": "https",
              "host": "*.youtube.com",
              "pathPrefix": "/watch"
            },
            {
              "scheme": "https",
              "host": "m.youtube.com",
              "pathPrefix": "/watch"
            },
            {
              "scheme": "https",
              "host": "youtu.be"
            }
          ],
          "category": [
            "BROWSABLE",
            "DEFAULT"
          ]
        },
        {
          "action": "SEND",
          "category": [
            "DEFAULT"
          ],
          "data": {
            "mimeType": "text/plain"
          },
          "label": "Summarize with YTSummarizer"
        },
        {
          "action": "PROCESS_TEXT",
          "category": [
            "DEFAULT"
          ],
          "data": {
            "mimeType": "text/plain"
          },
          "label": "Summarize with YTSummarizer"
        },
        {
          "action": "SEND",
          "category": [
            "DEFAULT"
          ],
          "data": {
            "mimeType": "text/*"
          },
          "label": "Summarize with YTSummarizer"
        },
        {
          "action": "SEND",
          "category": [
            "DEFAULT",
            "BROWSABLE"
          ],
          "data": {
            "mimeType": "*/*",
            "scheme": "https",
            "host": "*.youtube.com"
          },
          "label": "Summarize YouTube Video"
        },
        {
          "action": "SEND",
          "category": [
            "DEFAULT",
            "BROWSABLE",
            "ALTERNATIVE"
          ],
          "data": {
            "mimeType": "text/*",
            "scheme": "https",
            "host": "*.youtube.com"
          },
          "label": "Summarize YouTube Video"
        },
        {
          "action": "SEND",
          "category": [
            "DEFAULT"
          ],
          "data": {
            "mimeType": "*/*",
            "scheme": "https",
            "host": "m.youtube.com"
          },
          "label": "Summarize YouTube Video"
        },
        {
          "action": "SEND",
          "category": [
            "DEFAULT",
            "BROWSABLE"
          ],
          "data": {
            "mimeType": "*/*",
            "scheme": "https",
            "host": "youtu.be"
          },
          "label": "Summarize YouTube Video"
        },
        {
          "action": "SEND",
          "category": [
            "DEFAULT",
            "BROWSABLE"
          ],
          "data": {
            "mimeType": "text/*"
          },
          "label": "Summarize Text (YouTube Links)"
        },
        {
          "action": "SEND_MULTIPLE",
          "category": [
            "DEFAULT"
          ],
          "data": {
            "mimeType": "text/*"
          },
          "label": "Summarize Links"
        },
        {
          "action": "SEND",
          "category": [
            "DEFAULT"
          ],
          "data": {
            "mimeType": "text/plain",
            "scheme": "vnd.youtube"
          },
          "label": "Summarize YouTube Video"
        }
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "0001e79c-7e01-4762-b948-646cc2c9dc0a"
      }
    }
  }
}